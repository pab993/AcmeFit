<!-- Display report -->

<h3>
	<spring:message code="report.timeSpent" />: <jstl:out value="${report.getTimeSpent()}" />
</h3>

<h3>
	<spring:message code="report.kcalSpent" />: <jstl:out value="${report.getKcalSpent()}" />
</h3>

<h3>
	<spring:message code="report.mediumPulse" />: <jstl:out value="${report.getMediumPulse()}" />
</h3>

<h3>
	<spring:message code="report.exercisedMuscles" />: <jstl:out value="${report.getExercisedMuscles()}" />
</h3>

<h3>
	<spring:message code="report.score" />: <jstl:out value="${report.getScore()}" />
</h3>

<h3>
	<spring:message code="report.hints" />: <jstl:out value="${report.getHints()}" />
</h3>

